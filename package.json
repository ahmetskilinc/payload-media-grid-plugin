{
	"name": "payload-media-grid-plugin",
	"version": "0.0.12",
	"type": "module",
	"homepage:": "https://ahmetk.dev/",
	"repository": "git@github.com:ahmetskilinc/payload-media-grid-plugin.git",
	"description": "A Paylod CMS plugin to view your uploads as a gallery/grid instead of a list table.",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"keywords": [
		"payload",
		"cms",
		"media-grid",
		"media",
		"plugin",
		"typescript",
		"react"
	],
	"scripts": {
		"dev": "cd dev && cross-env NODE_OPTIONS=--no-deprecation next dev",
		"build": "tsc && pnpm copy",
		"test": "cd test && jest --config=./jest.config.js",
		"payload": "cd dev && cross-env NODE_OPTIONS=--no-deprecation payload",
		"generate:types": "payload generate:types",
		"copy": "copyfiles -u 1 \"src/**/*.css\" dist",
		"clean": "rimraf dist && rimraf dev/pnpm-lock.yaml",
		"prepublishOnly": "pnpm clean && pnpm build"
	},
	"author": "github.com/ahmetskilinc",
	"peerDependencies": {
		"payload": "3.0.0-beta.37"
	},
	"devDependencies": {
		"@payloadcms/db-mongodb": "3.0.0-beta.37",
		"@payloadcms/next": "3.0.0-beta.37",
		"@payloadcms/richtext-lexical": "3.0.0-beta.37",
		"@payloadcms/translations": "3.0.0-beta.37",
		"@payloadcms/ui": "3.0.0-beta.37",
		"@swc/jest": "0.2.36",
		"@types/jest": "29.5.12",
		"@types/node": "20.12.13",
		"@types/react": "npm:types-react@19.0.0-beta.2",
		"copyfiles": "^2.4.1",
		"cross-env": "7.0.3",
		"dotenv": "16.4.5",
		"jest": "29.7.0",
		"next": "15.0.0-rc.0",
		"payload": "3.0.0-beta.37",
		"prettier": "3.2.5",
		"react": "19.0.0-rc-935180c7e0-20240524",
		"rimraf": "^5.0.7",
		"sharp": "0.33.4",
		"typescript": "5.4.5"
	}
}