{
	"name": "payload-media-grid-plugin",
	"version": "0.0.5",
	"type": "module",
	"homepage:": "https://ahmetk.dev/",
	"repository": "git@github.com:ahmetskilinc/payload-media-grid-plugin.git",
	"description": "A Paylod CMS plugin to view your uploads as a gallery/grid instead of a list table.",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"keywords": [
		"payload",
		"cms",
		"media-grid",
		"media",
		"plugin",
		"typescript",
		"react"
	],
	"scripts": {
		"dev": "cd dev && cross-env NODE_OPTIONS=--no-deprecation next dev",
		"build": "tsc && pnpm copy",
		"test": "cd test && jest --config=./jest.config.js",
		"lint": "eslint src",
		"payload": "cd dev && cross-env NODE_OPTIONS=--no-deprecation payload",
		"dev:lint": "cd dev && cross-env NODE_OPTIONS=--no-deprecation next lint",
		"generate:types": "payload generate:types",
		"lint:fix": "eslint --fix --ext .ts,.tsx src",
		"copy": "copyfiles -u 1 \"src/**/*.css\" dist",
		"clean": "rimraf dist && rimraf dev/pnpm-lock.yaml",
		"prepublishOnly": "pnpm clean && pnpm build"
	},
	"author": "github.com/ahmetskilinc",
	"peerDependencies": {
		"payload": "3.0.0-beta.35"
	},
	"devDependencies": {
		"@payloadcms/db-mongodb": "3.0.0-beta.35",
		"@payloadcms/eslint-config": "3.0.0-beta.16",
		"@payloadcms/next": "3.0.0-beta.35",
		"@payloadcms/richtext-lexical": "3.0.0-beta.35",
		"@payloadcms/ui": "3.0.0-beta.35",
		"@swc/jest": "0.2.36",
		"@types/jest": "29.5.12",
		"@types/node": "20.12.12",
		"@types/react": "18.3.2",
		"@typescript-eslint/eslint-plugin": "7.9.0",
		"@typescript-eslint/parser": "7.9.0",
		"copyfiles": "^2.4.1",
		"cross-env": "7.0.3",
		"dotenv": "16.4.5",
		"eslint": "9.2.0",
		"eslint-config-airbnb-base": "15.0.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-import": "2.29.1",
		"eslint-plugin-prettier": "5.1.3",
		"jest": "29.7.0",
		"next": "14.3.0-canary.68",
		"payload": "3.0.0-beta.35",
		"prettier": "3.2.5",
		"react": "18.3.1",
		"rimraf": "^5.0.7",
		"sharp": "0.33.4",
		"typescript": "5.4.5"
	},
	"dependencies": {
		"@payloadcms/translations": "3.0.0-beta.35"
	}
}